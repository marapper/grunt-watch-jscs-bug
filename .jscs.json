{
    "disallowTrailingComma": true,

    "additionalRules": [
        "node_modules/jscs-rules/src/*.js"
    ],
    "requireFunctionsContent": {
        "initialize": {
            "regexpText": "(this\\._super|origin\\.{functionName}|\\.prototype\\.{functionName})",
            "error": "required call method \"this._super\""
        },
        "_initProperties": {
            "regexpText": "(this\\._super|origin\\.{functionName}|\\.prototype\\.{functionName})",
            "error": "required call method \"this._super\""
        }
    }
}
